задание 1 
#включить <stdio.h>
int main() {
 int a,b,c;
 printf("Введите a");
 scanf("%i",&a );
 printf("Введите b");
 scanf("%i",&b );
 c = a;
 a = b;
 b = c;
 printf("a= %i,b = %i ",a,b);
 возврат 0 ;
}
