задание 1 
#include <stdio.h>
int main() {
 int a,b,c;
 printf("Enter a");
 scanf("%i",&a );
 printf("Enter b");
 scanf("%i",&b );
 c = a;
 a = b;
 b = c;
 printf("a= %i,b = %i ",a,b);
 return 0 ;
}
задание 2
#include <stdio.h>
int main() {
    int a,b,c,dop,dop1 ;
    printf("Enter a");
    scanf("%i",&a  );
    printf("Enter b");
    scanf("%i",&b );
    printf("Enter c");
    scanf("%i",&c );
    dop1 = b;
    b = a;
    a = c;
    c = dop1;
    printf("a= %i,b = %i, c = %i ",a,b,c);
    return 0 ;
}
задание 3
#include <stdio.h>
int main() {
    int a,b,c,dop,dop1 ;
    printf("Enter a");
    scanf("%i",&a  );
    printf("Enter b");
    scanf("%i",&b );
    printf("Enter c");
    scanf("%i",&c );
    dop1 = c;
    c = a;
    a = b;
    b = dop1;
    printf("a= %i,b = %i, c = %i ",a,b,c);
    return 0 ;
}
задание 4
#include <stdio.h>
#include <math.h>
int main() {
int x;
float pr;
printf("Enter x ");
scanf("%i",&x);
pr = 3 * pow(x,6)-6*pow(x,2)-7;
printf("composition = %f", pr);
return 0;
}
задание 5
#include <stdio.h>
#include <math.h>
int main() {
int x;
float y;
printf("Enter x");
scanf("%i",&x);
y = 4 * pow(x-3,6)-7*pow(x-3,3)+2;
printf("y = %f", y);
return 0;
}
задание 6
#include <stdio.h>
#include <math.h>
int main(){
int a,a8,p;
printf("enter a");
scanf("%i",&a);
p = 2;
p = p*p*p;
a8= pow(a,p);
printf("a= %i",a8);
return 0 ;
}
задание 7
#include <stdio.h>
#include <math.h>
int main() {
float a,a15,p,p1;
printf ("enter A");
scanf ("%f",&a);
p1 = a*a; //2p1
printf("%f\n",p1);
p= p1*a; //3p
printf("%f\n",p);
p= p1*p;//5p
printf("%f\n",p1);
a15 = p*p*p;
printf("the number A to the power of 15 is equal to %f ", a15);
return 0;
}
